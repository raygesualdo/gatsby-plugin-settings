{
  "name": "@raygesualdo/gatsby-plugin-settings",
  "version": "0.0.0",
  "description": "Gatsby plugin to manage user settings more effectively",
  "scripts": {
    "cm": "git-cz",
    "semantic-release": "semantic-release",
    "precommit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/raygesualdo/gatsby-plugin-settings.git"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin"
  ],
  "author": "Ray Gesualdo <ray@raygesualdo.com> (https://raygesualdo.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/raygesualdo/gatsby-plugin-settings/issues"
  },
  "homepage": "https://github.com/raygesualdo/gatsby-plugin-settings#readme",
  "dependencies": {
    "chokidar": "^2.0.2",
    "gatsby-cli": "^1.1.45",
    "glob": "^7.1.2",
    "js-yaml": "^3.11.0",
    "lodash": "^4.17.5",
    "toml": "^2.3.3"
  },
  "devDependencies": {
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "prettier": "^1.11.1",
    "semantic-release": "^15.0.3"
  },
  "lint-staged": {
    "**/*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
